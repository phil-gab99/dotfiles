#include <iostream>
#include <vector>

using namespace std;

class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int i, j;
        for (i = 0; i < nums.size(); i++) {
            if (nums[i] > target) continue;
            for (j = i + 1; j < nums.size(); j++)
                if (nums[i] + nums[j] == target) return vector<int>({i, j});
        }
        return vector<int>();
    }
};

int main() {
    Solution s;
    vector<int> var({2, 7, 11, 15});
    vector<int> res = s.twoSum(var, 9);
    cout << "Solution: ";
    for (auto const &value : res) {
        cout << value << " ";
    }
    return 0;
}

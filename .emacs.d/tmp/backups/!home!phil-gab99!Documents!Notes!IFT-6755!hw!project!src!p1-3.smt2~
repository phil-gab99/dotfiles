(declare-const Wash Bool)
(declare-const Delay Bool)
(declare-const Heat Bool)
(declare-const Dry Bool)

;; Feature Model constraints
(assert Wash)
(assert (not (and Delay Heat)))
(assert (or Dry (not Dry)))

;; Negation of affirmation
(assert (not (not (and Delay Heat))))
(check-sat)
;; Unsat therefore encoding does not violate mutual exclusion property

MODULE main
VAR
    state: {s0, s1, s2, s3};
ASSIGN
    init(state) := s0; -- Initial state
    next(state) :=
        case
          state = s0: {s0, s1, s3};
          state = s1: {s2}; 
          state = s2: {s1};
          state = s3: {s2};
        esac;
DEFINE
    p := state = s1 | state = s3;
    q := state = s2 | state = s3;
    r := state = s0 | state = s1 | state = s2;
    t := state = s1;
SPEC EF q

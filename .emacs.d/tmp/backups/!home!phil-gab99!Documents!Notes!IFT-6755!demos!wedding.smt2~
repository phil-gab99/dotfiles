;; Aunt
(declare-const x11 Bool)
(declare-const x12 Bool)
(declare-const x13 Bool)

;; Sister
(declare-const x21 Bool)
(declare-const x22 Bool)
(declare-const x23 Bool)

;; Father
(declare-const x31 Bool)
(declare-const x32 Bool)
(declare-const x33 Bool)

;; Aunt no want near Father
(assert
 (and
  (=> (or x11 x13) (not x32))
  (=> x12 (and (not x31) (not x33)))))

;; Aunt no want sit in left chair
(assert (not x11))

;; Sister no want sit right of Father
(assert
 (and
  (=> x31 (not x22))
  (=> x32 (not x23))))

;; Each is placed
(assert
 (and (or x11 x12 x13) (or x21 x22 x23) (or x31 x32 x33)))

;; No one placed in more than one place
(assert
 (and (and (and (or (not x11) (not x12))
                (or (not x11) (not x13)))
           (and (or (not x12) (not x13))))

      (and (and (or (not x21) (not x22))
                (or (not x21) (not x23)))
           (and (or (not x22) (not x23))))
      
      (and (and (or (not x31) (not x32))
                (or (not x31) (not x33)))
           (and (or (not x32) (not x33))))))

;; No one in same place
(assert
 (and
  (not (or (and x11 x21) (and x11 x31) (and x21 x31)))
  (not (or (and x12 x22) (and x12 x32) (and x22 x32)))
  (not (or (and x13 x23) (and x13 x33) (and x23 x33)))))

(check-sat)
(get-model)

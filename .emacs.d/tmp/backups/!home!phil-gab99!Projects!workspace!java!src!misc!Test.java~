package misc;

import java.util.ArrayList;
import java.util.List;

public class Test {
    
    static List<String> subs = new ArrayList<>();
    static String sequence = "GTACA";
    
    public static void main(String[] args) {
        
        findsubsequences(sequence, "");
        System.out.println(subs);
    }
    
    private static void findsubsequences(String s, String ans) {
        
        if (s.length() == 0) {
            
            // if (ans.length() < sequence.length() && ans.length() > 0) {
            // 
            //     subs.add(ans);
            // }
            subs.add(ans);
            return;
        }

        findsubsequences(s.substring(1), ans.length() < sequence.length() ? ans + s.charAt(0) : ans);
        
        findsubsequences(s.substring(1), ans);
    }
}
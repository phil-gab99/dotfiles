        .data
array:  .word       2, 3, 5, 7, 42
size:   .word       5

        .text
main:   la          $a0, array
        lw          $a1, size
        jal         find42

print:  move        $a0, $v0
        li          $v0, 1
        syscall

exit:   li          $v0, 10
        syscall

find42: li          $v0, 0
for:    bge         $v0, $a1, return1
        sll         $t0, $v0, 2
        add         $t0, $a0, $t0
        lw          $t0, ($t0)
        beq         $t0, 42, return2
        addi        $v0, $v0, 1
        j           for

return1:li          $v0, âˆ’1
return2:jr          $ra

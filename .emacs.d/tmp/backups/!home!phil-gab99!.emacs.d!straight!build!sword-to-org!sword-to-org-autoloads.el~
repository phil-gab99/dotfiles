;;;### (autoloads nil "sword-to-org" "sword-to-org.el" (0 0 0 0))
;;; Generated autoloads from sword-to-org.el

(autoload 'sword-to-org-insert-outline "sword-to-org" "\
Insert Org outline in current buffer for Sword MODULE and KEY.
The buffer will be switched to `text-mode' before inserting, to
improve performance, and then switched back to `org-mode' if
it was active.

\(fn MODULE KEY)" t nil)

(autoload 'sword-to-org-insert-passage "sword-to-org" "\
Insert passage for reference KEY as plain text.
With prefix, prompt for module, otherwise use default module.
With double-prefix, insert each verse on its own line with
reference; otherwise, insert as single paragraph with reference
at the end.

\(fn KEY &optional SEPARATE-LINES MODULE)" t nil)

(autoload 'org-sword-search "sword-to-org" "\
Search Diatheke for KEY in MODULE and present the passages as an Org buffer.

\(fn KEY &optional MODULE)" t nil)

(register-definition-prefixes "sword-to-org" '("org-sword--" "sword-to-org-"))


# fib(0) = 0, fib(1) = 1
        li          $a0, 6
        jal         fib
        move        $a0, $v0
        li          $v0, 1
        syscall
        li          $v0, 10
        syscall
        
fib:    subi        $sp, $sp, 8
        sw          $ra, 4($sp)
        sw          $a0, 0($sp)

        bgt         $a0, 1, rec
        move        $v0, $a0
        j           return
        
rec:    subi        $a0, $a0, 1
        jal         fib

        subi        $sp, $sp, 4
        sw          $v0, 0($sp)

        subi        $a0, $a0, 1
        jal         fib

        lw          $t0, 0($sp)
        addi        $sp, $sp, 4
        add         $v0, $v0, $t0
        
return: lw          $a0, 0($sp)
        lw          $ra, 4($sp)
        addi        $sp, $sp, 8
        jr          $ra
